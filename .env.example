# Przykładowy plik konfiguracyjny
# Skopiuj ten plik do .env i uzupełnij wartości

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Langfuse API Keys
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Server Configuration
NODE_ENV=development
PORT=3000
MONGODB_URI=mongodb://localhost:27017/cinehub
MONGODB_TEST_URI=mongodb://localhost:27017/cinehub_test
JWT_SECRET=your_jwt_secret_here
API_RATE_LIMIT=100
API_RATE_WINDOW=15

# Weaviate Configuration
WEAVIATE_URL=your_weaviate_url_here
WEAVIATE_API_KEY=your_weaviate_api_key_here

# Other API Keys
# Add your actual API keys here when copying to .env
